{
    "mcpServers": {
        "supabase": {
            "type": "http",
            "url": "https://mcp.supabase.com/mcp?project_ref=YOUR_PROJECT_REF",
            "headers": {
                "Authorization": "Bearer YOUR_SUPABASE_ACCESS_TOKEN"
            }
        },
        "party-linear": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "ts-node",
                "--esm",
                "scripts/linear-mcp.ts"
            ],
            "env": {
                "LINEAR_API_KEY": "YOUR_LINEAR_API_KEY"
            }
        }
    }
}